    <script>
        let path = window.location.origin;
        $(document).ready(() => {
            $(document).on('click', '.respond', e => {
                let friendid = ($(e.target).attr('class').split(' ')[0]);
                $.ajax({
                    type: 'POST',
                    url: path + '/user/approvefriend',
                    data: {
                        friendid
                    }, success: (response) => {
                        // $(e.target).css('display','none');
                        $(e.target).removeClass('fa-user-plus');
                        // $(e.target).removeClass('respond');
                        $(e.target).addClass('fa-check');
                        // $(e.target).addClass('unfriend');
                        $(e.target).css('color', '#8ec68e');
                        $(e.target).attr('title', 'accepted');
                        // console.log('approved');
                    }
                })

            })
            
        })

    </script>
    <script>
        function searchName() {
            var mess = (document.getElementById('search').value);
            mess=mess.toUpperCase();
            if (mess != '') {
                var i = 0;
                while (document.getElementsByClassName('friendbook')[i]) {
                    // console.log(document.getElementsByClassName('friendname')[i].innerHTML, document.getElementsByClassName('friendname')[i].innerHTML.indexOf(mess));
                    if (document.getElementsByClassName('friendname')[i].innerHTML.toUpperCase().indexOf(mess) != -1 || document.getElementsByClassName('bookname')[i].innerHTML.toUpperCase().indexOf(mess) != -1) {
                        document.getElementsByClassName('friendbook')[i].style.display = '';

                    }
                    else {
                        document.getElementsByClassName('friendbook')[i].style.display = 'none';
                    }
                    i = i + 1;
                }
            }
            else {
                var i = 0;
                while (document.getElementsByClassName('friendname')[i]) {
                    document.getElementsByClassName('friendbook')[i].style.display = '';

                    i = i + 1;
                }
            }
        }
    </script>
