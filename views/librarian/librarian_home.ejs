    <script>
        let path = window.location.origin;
        $('form').submit(e => {
            e.preventDefault();
        })
        $(document).ready(() => {
            $('#collectfine').submit(e => {
                console.log($('#collectfine').serialize());
                e.preventDefault();
                $.ajax({
                    type: 'POST',
                    url: path + '/librarian/fine',
                    data: {
                        data: $('#collectfine').serialize(),
                    },
                    success: (resp) => {
                        if (resp.message.length) {
                            
                            $('.finemess').html(`<div class="alert alert-success"><p>${resp.message}</p></div>`);
                        } else {
                            
                            $('.finemess').html(`<div class="alert alert-danger"><p>${resp.error}</p></div>`);
                        }
                    }
                })
            })

            $('#withdrawbook').submit(e => {
                let fdata = $('#withdrawbook').serialize();
                console.log(fdata);
                e.preventDefault();
                $.ajax({
                    type: 'POST',
                    url: path + '/librarian/withdrawbook',
                    data: {
                        data: fdata
                    }, success: (resp) => {
                        if (resp.message.length) {
                            $('.withdrawmess').html(`<div class="alert alert-success"><p>${resp.message}</p></div>`);
                        } else {
                            $('.withdrawmess').html(`<div class="alert alert-danger"><p>${resp.error}</p></div>`);
                        }
                        console.log('withdrawn!');
                    }
                })
            })
            $('#issuebook').submit(e => {
                let fdata = $('#issuebook').serialize();
                console.log(fdata);
                e.preventDefault();
                $.ajax({
                    type: 'POST',
                    url: path + '/librarian/issuebook',
                    data: {
                        data: fdata
                    }, success: (resp) => {
                        if (resp.message.length) {
                            $('.issuemess').html(`<div class="alert alert-success"><p>${resp.message}</p></div>`);
                        } else {
                            $('.issuemess').html(`<div class="alert alert-danger"><p>${resp.error}</p></div>`);
                        }
                        console.log('book issued!');
                    }
                })
            })
            $('#updatebook').submit(e => {
                let fdata = $('#updatebook').serialize();
                console.log(fdata);
                e.preventDefault();
                $.ajax({
                    type: 'POST',
                    url: path + '/librarian/updatebook',
                    data: {
                        data: fdata
                    }, success: (resp) => {
                        if (resp.message.length) {
                            $('.updatemess').html(`<div class="alert alert-success"><p>${resp.message}</p></div>`);
                        } else {
                            $('.updatemess').html(`<div class="alert alert-danger"><p>${resp.error}</p></div>`);
                        }
                        console.log('book updated!');
                    }
                })
            })
            $('#deletebook').submit(e => {
                let fdata = $('#deletebook').serialize();
                console.log(fdata);
                e.preventDefault();
                $.ajax({
                    type: 'POST',
                    url: path + '/librarian/deletebook',
                    data: {
                        data: fdata
                    }, success: (resp) => {
                        if (resp.message.length) {
                            $('.deletemess').html(`<div class="alert alert-success"><p>${resp.message}</p></div>`);
                        } else {
                            $('.deletemess').html(`<div class="alert alert-danger"><p>${resp.error}</p></div>`);
                        }
                        console.log('book deleted!');
                    }
                })
            })
            $('#addbook').submit(e => {
                let fdata = $('#addbook').serialize();
                console.log(fdata);
                e.preventDefault();
                $.ajax({
                    type: 'POST',
                    url: path + '/librarian/addbook',
                    data: {
                        data: fdata
                    }, success: (resp) => {
                        if (resp.message.length) {
                            $('.addmess').html(`<div class="alert alert-success"><p>${resp.message}</p></div>`);
                        } else {
                            $('.addmess').html(`<div class="alert alert-danger"><p>${resp.error}</p></div>`);
                        }
                        console.log('book added!');
                    }
                })
            })
            $('.viewfine').click(()=>{
                let email = $('#fineemail').val();
                $.ajax({
                    type : 'POST',
                    url : path+'/librarian/viewfine',
                    data : {
                        email
                    },
                    success : (resp)=>{
                        $('.finemess').removeClass('alert alert-primary alert-danger');
                        if(resp.message.length){
                            $('.finemess').html(`<div class="alert alert-success"><p>${resp.message}</p></div>`);
                        }else{
                            $('.finemess').html(`<div class="alert alert-danger"><p>${resp.error}</p></div>`);
                        }

                    }
                })
            })
        })
    </script>
